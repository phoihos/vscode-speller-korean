{
  "name": "speller-korean",
  "displayName": "Speller for Korean",
  "description": "Korean spell checker, Correct korean spell",
  "icon": "images/icon.png",
  "publisher": "phoihos",
  "repository": {
    "type": "git",
    "url": "https://github.com/phoihos/vscode-speller-korean"
  },
  "bugs": {
    "url": "https://github.com/phoihos/vscode-speller-korean/issues"
  },
  "homepage": "https://github.com/phoihos/vscode-speller-korean#readme",
  "license": "MIT",
  "version": "1.6.1",
  "engines": {
    "vscode": "^1.49.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "korean",
    "grammar",
    "grammar-checker",
    "proofread",
    "speller"
  ],
  "activationEvents": [
    "onCommand:speller.correct",
    "onCommand:speller.correct.document",
    "onCommand:speller.correct.selection"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "speller.correct",
        "title": "Correct Selection or Document",
        "category": "Speller-Korean"
      },
      {
        "command": "speller.correct.document",
        "title": "Correct Korean Spell in Document",
        "category": "Speller-Korean"
      },
      {
        "command": "speller.correct.selection",
        "title": "Correct Korean Spell in Selection",
        "category": "Speller-Korean"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "speller.correct.document",
          "when": "false"
        },
        {
          "command": "speller.correct.selection",
          "when": "false"
        }
      ],
      "editor/context": [
        {
          "command": "speller.correct.document",
          "group": "1_modification"
        },
        {
          "command": "speller.correct.selection",
          "when": "editorHasSelection",
          "group": "1_modification"
        }
      ]
    },
    "keybindings": [
      {
        "command": "speller.correct",
        "key": "shift+alt+k"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^14.18.21",
    "@types/vscode": "^1.49.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@phoihos/vsce-util": "https://github.com/phoihos/vsce-util.git#v0.0.1",
    "typed-rest-client": "^1.8.9"
  }
}
